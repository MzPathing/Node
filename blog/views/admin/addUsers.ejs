<!--增加用户页面-->
<h1 class="welcome-title">增加用户</h1>
<div class="container">
	<div class="row">
		<form class="navbar-form" action="addUsers" method="post">
			<div class="input-group col-lg-4 col-xs-6 col-xs-offset-2 user-info">
			  <span class="input-group-addon"><span class="glyphicon glyphicon-star text-info"></span></span>
			  <input id="username" type="text" class="form-control " name="username" placeholder="请输入新增用户">
			</div>
			<div class="clearfix"></div>
			<div class="input-group col-lg-4 col-xs-6 col-xs-offset-2 user-info">
			  <span class="input-group-addon"><span class="glyphicon glyphicon-list-alt text-info"></span></span>
			  <input id="username" type="text" class="form-control " name="password" placeholder="请输入密码">
			</div>
			<div class="clearfix"></div>
			<div class="input-group col-lg-4 col-xs-6 col-xs-offset-2 user-info">
			  <span class="input-group-addon"><span class="glyphicon glyphicon-list-alt text-info"></span></span>
			  <input id="username" type="text" class="form-control " name="repassword" placeholder="请确认密码">
			</div>
			<div class="clearfix"></div>
			<div class="input-group col-lg-4 col-xs-6 col-xs-offset-2 user-info">
			  <span class="input-group-addon"><span class="glyphicon glyphicon-list-alt text-info"></span></span>
			  <input id="username" type="text" class="form-control " name="contact" placeholder="请输入联系方式">
			</div>
			<div class="clearfix"></div>



			<div class="col-lg-3 col-lg-offset-3">
				<button id="signup" class="btn">提交</button>
				<button id="reset" class="btn">重置</button>
			</div>
		</form>
	</div>
</div>